<component id='ID_s2_title'>
	<text><![CDATA[<b>Access Level</b>]]></text>
</component>

	<component id="ID_s2_table"><text><![CDATA[
	<table header='none' bordercolor='#FFFFFF' borderwidth='3' shadow='false' shadowcolor='#000000' glow='true' glowcolor='#808080' cellpadding='4'>
		<tr class="mtable_title">
			<td colspan="2" bgcolor='#FFFFFF'><font size="16"><br/> </font></td>
		</tr>
		
		<tr class="mtable_top">
			<td colspan='2' bgcolor='#4E4C4D'><font size='1' color='#4E4C4D'>.</font></td>
		</tr>
		
		<tr>
			<td bgcolor='#666263'  valign="top"  width="145"><font color='#FFFFFF' ><b>Access Level</b></font></td>
			<td bgcolor='#666263'  valign="top"  ><font color='#FFFFFF' ><b>Level Description</b></font></td>
			
		</tr>
		
		<tr>
			<td bgcolor='#E9E7E8'><b>Full Access</b></td>
			<td bgcolor='#E9E7E8'><textformat blockindent='-14'><ul><li>All screens and settings are available, except the supervisor password setting.</li></ul></textformat></td>
			
		</tr>
		<tr>
			<td bgcolor='#E9E7E8'><b>Limited Access</b></td>
			<td bgcolor='#E9E7E8'><textformat blockindent='-14'><ul><li>Changes can be made to certain settings only, for example, the time and date.</li></ul></textformat></td>
			
		</tr>
		<tr>
			<td bgcolor='#E9E7E8'><b>View Only Access</b></td>
			<td bgcolor='#E9E7E8'><textformat blockindent='-14'><ul><li>All screens are available, but no settings can be changed.</li></ul></textformat></td>
			
		</tr>
		<tr>
			<td bgcolor='#E9E7E8'><b>No Access</b></td>
			<td bgcolor='#E9E7E8'><textformat blockindent='-14'><ul><li>No access is provided to the BIOS setup utility.</li></ul></textformat></td>
			
		</tr>
		
		
	</table>]]></text> 
</component>

